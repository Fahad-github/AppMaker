<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <androidx.drawerlayout.widget.DrawerLayout
        android:layout_width="match_parent"
        android:id="@+id/template2Drawer"
        android:fitsSystemWindows="true"
        android:layout_height="match_parent"
        tools:openDrawer="start"
        tools:context=".Functionality.Template2">

        <include
            layout="@layout/app_bar_template2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </include>

        <com.google.android.material.navigation.NavigationView
            android:id="@+id/template2NavigationDrawer"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            app:menu="@menu/template2_menu"
            android:fitsSystemWindows="true"
            app:headerLayout="@layout/nav_header_template2"
            >

        </com.google.android.material.navigation.NavigationView>

    </androidx.drawerlayout.widget.DrawerLayout>

</layout>